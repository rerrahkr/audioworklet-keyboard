{
	"$schema": "https://biomejs.dev/schemas/1.7.3/schema.json",
	"files": {
    "ignore": [
      "dist/",
      "*.md"
    ]
  },
  "organizeImports": {
		"enabled": true
	},
  "formatter": {
    "indentStyle":"space"
  },
  "javascript": {
    "formatter": {
      "trailingComma": "es5"
    }
  },
	"linter": {
		"enabled": true,
		"rules": {
			"style": {
        "noDefaultExport": "off"
      }
		}
	}
}
